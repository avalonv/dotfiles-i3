!! https://github.com/beingmohit/dotfiles/blob/master/config/rofi/config

rofi.theme: ~/.config/rofi/17script.me
! Terminal to use
!rofi.terminal:                       rofi-sensible-terminal
rofi.terminal:                       urxvt
! Ssh client to use
rofi.ssh-client:                     ssh
! Ssh command to execute
rofi.ssh-command:                    {terminal} -e {ssh-client} {host}
! Run command to execute
rofi.run-command:                    {cmd}
! Command to get extra run targets
rofi.run-list-command:
! Run command to execute that runs in shell
rofi.run-shell-command: {terminal} -e {cmd}

rofi.bw: 2
rofi.opacity: 40

!!!! https://github.com/mightysilentsaint/stuff/blob/master/dotfiles/home/mss/.config/rofi/config
!!COLORS		        bg    				fg    		activeb		ACTIVE	 	hlfg
